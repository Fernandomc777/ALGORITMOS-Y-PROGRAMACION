//Algoritmo que calcula el área de un rectángulo.
Algoritmo SEC05
	Definir Largo, Ancho Como Real
	
	Escribir "Introduzca el largo: ";
	Leer Largo;
	Escribir  "Introduzca el ancho: ";
	Leer Ancho;
	
	Escribir "El área del rectángulo es: ", Largo * Ancho;
	
FinAlgoritmo
